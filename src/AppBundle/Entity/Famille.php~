<?php

namespace VB\RecetteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Famille
 *
 * @ORM\Table(name="famille")
 * @ORM\Entity
 */
class Famille
{
    /**
     * @var string
     *
     * @ORM\Column(name="nom_famille", type="string", length=25)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $nomFamille;

    /**
     * @var \Doctrine\Common\Collections\Collection
     *
     * @ORM\ManyToMany(targetEntity="VB\RecetteBundle\Entity\Categorie", mappedBy="nomFamille")
     */
    private $nomCategorie;

    /**
     * Constructor
     */
    public function __construct()
    {
        $this->nomCategorie = new \Doctrine\Common\Collections\ArrayCollection();
    }

}

